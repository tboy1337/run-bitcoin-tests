[run]
source = .
omit =
    */tests/*
    */test_*
    *_test.py
    */venv/*
    */.venv/*
    */__pycache__/*
    */build/*
    */dist/*
    .tox/*
    .coverage
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml